<Trainee>
    canvas.before:
        Color:
            rgba: 1, 1, 1, .6
        Rectangle:
            pos: self.pos
            size: self.size
            source: 'img/dungeon.jpeg'
    canvas:
        Color:
            rgba: 0.8, 1, 0.6, 1
        Triangle:
            points: 280,250, 280,300, 300,275
        Triangle:
            points: 500,250, 500,300, 520,275
    BoxLayout:
        orientation: 'vertical'
        ActionBar:
            ActionView:
                ActionPrevious:
                    title: 'Trainee'
                    on_release:
                        app.root.transition.duration = 1.5
                        app.root.transition.direction = 'down'
                        app.root.current = 'MenuTutorial'
                ActionSeparator:
                ActionButton:
                    text: 'Mute'
                    on_release:
                        app.root.get_screen('MenuTutorial').menu_som.stop()
                        app.root.get_screen('MenuTutorial').menu_som.loop = False
                ActionSeparator:
                ActionButton:
                    text: 'Play'
                    on_release:
                        app.root.get_screen('MenuTutorial').menu_som.play()
                        app.root.get_screen('MenuTutorial').menu_som.loop = True
                ActionSeparator:
                ActionButton:
                    text: 'Sair'
                    on_release: app.root.get_screen('menual').confirmacaosaida()
        BoxLayout:
            padding: 100
            spacing: 50
            BotaoAutocad:
                on_release:
                    root.autocad()
            BotaoGerencia:
                on_release:
                    root.gerencia_de_projetos()
            BotaoProjetos:
                on_release:
                    root.projeto_ficticio()

