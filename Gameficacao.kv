<DPJ>:
	BoxLayout:
		orientation: 'vertical'
		ActionBar:
			ActionView:
				ActionPrevious:
					title: 'DPJ'
					on_release:
					    root.manager.transition = FadeTransition()
					    root.manager.transition.duration = 1.5
						app.root.current = 'menuG'
						root.manager.transition = SlideTransition()
				ActionButton:
					text: 'Sair'
					on_release: app.root.get_screen('menual').confirmacaosaida()

		BoxLayout:
			orientation: 'horizontal'
			padding: 20
			spacing: 5
			canvas:
			    Color:
                    rgba: 1, 1, 1, 0.6
				Rectangle:
					source: 'img/connor-wakes-conwak-sotn.jpg'
					size: self.size

			Image:
				source: 'img/IMG_20200119_124314.jpg'
				size: (200, 500)

			BoxLayout:
				orientation: 'vertical'
				padding: 10
				Label:
					text: 'Departamento\n     de\n  Projetos'
					font_size: 35
					font_name: 'font/joystix monospace.ttf'
				BoxLayout:
					padding: 10
					Image:
						source: root.img
						size: self.size
						allow_stretch: True

